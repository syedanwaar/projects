      "cell_type": "code",
      "source": [
        "\n",
        "import re\n",
        "\n",
        "StopWords=[]\n",
        "\n",
        "with open('stop_words.txt','r') as SW:\n",
        "    StopWordString=list(SW)[0].split('\\n')[0]\n",
        "    filteredChars = re.findall(r'\\w+', StopWordString)\n",
        "    StopWords=filteredChars.copy()\n",
        "    StopWordsLower = [x.lower() for x in StopWords]\n",
        "print(StopWordsLower)"
      ],
      "metadata": {
