      "cell_type": "code",
      "source": [
        "\n",
        "ref=dict() \n",
        "for i in range(1,101):\n",
        "  with open(f'files/{i}.txt','r') as reviewfile:\n",
        "    review=list(reviewfile)[0].split('\\n')[0]\n",
        "    res = re.findall(r'\\w+', review)\n",
        "    res_without_sw=[]\n",
        "    for word in res:\n",
        "       if not word.lower() in StopWordsLower:\n",
        "         res_without_sw.append(word)\n",
        "    for word in res_without_sw:\n",
        "      if word not in ref:\n",
        "        ref[word]={i}\n",
        "      else:\n",
        "        ref[word].add(i)\n",
        "print(ref)"
